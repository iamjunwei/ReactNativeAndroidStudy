            </intent-filter>
        </activity>
        <activity android:name="com.facebook.react.devsupport.DevSettingsActivity" />
        <activity android:name=".ListExampleActivity"/>
        <activity android:name=".MovieActivity"/>
        <activity android:name=".ImageExampleActivity"/>
        <activity android:name=".DrawerLayoutExampleActivity"/>
        <activity android:name=".TouchableHighLightExampleActivity"/>
        <activity android:name=".TouchableNativeFeedbackExampleActivity"/>
        <activity android:name=".TouchableOpacityExampleActivity"/>
        <activity android:name=".ViewPagerExampleActivity"/>
        <activity android:name=".MyMenuActivity"/>
        <activity android:name=".TabHostExampleActivity"/>
        <activity android:name=".LayoutViewExampleActivity"/>
